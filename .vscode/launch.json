        {
            "name": "attach knkMedia docker",
            "request": "attach",
            "type": "al",
            "environmentType": "OnPrem",
            "server": "http://knkMedia/BC",
            "serverInstance": "BC",
            "authentication": "UserPassword",
            "breakOnError": "All",
            "breakOnNext": "WebClient",
